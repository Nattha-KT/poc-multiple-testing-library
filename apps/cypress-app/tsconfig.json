{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["DOM", "ES6"],
    "types": ["cypress"],
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["cypress/**/*.ts", "cypress/**/*.tsx"]
}
